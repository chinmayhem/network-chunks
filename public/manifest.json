{
  "name": "Network Chunks",
  "description": "Track the chunks",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Network Chunks"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.sprinklr.com/*"
      ],
      "js": [
        "./static/js/content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "host_permissions": [
    "https://*.sprinklr.com/*"
  ],
  "background": {
    "service_worker": "./static/js/background.js"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "permissions": [
    "storage",
    "webRequest",
    "tabs"
  ]
}